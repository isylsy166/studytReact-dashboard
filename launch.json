{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome against localhost",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}",
            "sourceMaps": true,
            "autoAttach": true,
            "runtimeOptions": [
                "--disable-extensions"
            ]
        }
    ]
}